<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="AnchorPane" prefHeight="661.0" prefWidth="542.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="icare.controllers.ViewEditController">
  <children>
    <Label layoutX="206.0" layoutY="29.0" prefHeight="42.0" prefWidth="272.0" text="'s Medical Record">
      <font>
        <Font size="20.0" fx:id="x1" />
      </font>
    </Label>
    <Label fx:id="userTitleLbl" alignment="CENTER_RIGHT" font="$x1" layoutX="14.0" layoutY="29.0" prefHeight="42.0" prefWidth="192.0" text="[user]" />
    <Pane layoutX="26.0" layoutY="97.0" prefHeight="235.0" prefWidth="471.0">
      <children>
        <ListView fx:id="listView" layoutX="42.0" layoutY="35.0" onMouseClicked="#userClickedListView" prefHeight="200.0" prefWidth="307.0" />
        <Label layoutX="168.0" layoutY="13.0" text="Diseases" />
        <Button layoutX="368.0" layoutY="90.0" mnemonicParsing="false" onAction="#addDiseaseClicked" prefHeight="27.0" prefWidth="115.0" text="Add Disease" />
        <Button id="deleteBtn" fx:id="deleteDiseaseBtn" disable="true" layoutX="368.0" layoutY="135.0" mnemonicParsing="false" onAction="#deleteDiseaseBtnClicked" prefHeight="27.0" prefWidth="115.0" text="Delete Disease" />
      </children>
    </Pane>
    <Pane layoutX="26.0" layoutY="342.0" prefHeight="235.0" prefWidth="495.0">
      <children>
        <TableView fx:id="tableView" layoutX="42.0" layoutY="35.0" onMouseClicked="#userClickedTableView" prefHeight="200.0" prefWidth="307.0" tableMenuButtonVisible="false">
          <columns>
            <TableColumn fx:id="instructionsCol" maxWidth="5000.0" minWidth="10.0" prefWidth="130.0" text="Instructions" />
            <TableColumn fx:id="medicationCol" maxWidth="5000.0" minWidth="10.0" prefWidth="102.0" text="Medication" />
            <TableColumn fx:id="weeksCol" maxWidth="5000.0" minWidth="10.0" prefWidth="86.0" text="# of Weeks" />
          </columns>
        </TableView>
        <Label layoutX="168.0" layoutY="13.0" text="Treatments" />
        <Button layoutX="368.0" layoutY="90.0" mnemonicParsing="false" onAction="#addTreatmentClicked" prefHeight="27.0" prefWidth="128.0" text="Add Treatment" />
        <Button id="deleteBtn" fx:id="deleteTreatmentBtn" disable="true" layoutX="368.0" layoutY="135.0" mnemonicParsing="false" onAction="#deleteTreatmentBtnClicked" prefHeight="27.0" prefWidth="128.0" text="Delete Treatment" />
      </children>
    </Pane>
      <Label fx:id="warningLbl" alignment="CENTER" layoutX="4.0" layoutY="585.0" prefHeight="17.0" prefWidth="534.0" textFill="RED">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="436.0" layoutY="610.0" mnemonicParsing="false" onAction="#saveBtnClicked" prefHeight="27.0" prefWidth="61.0" text="Save" />
      <Button layoutX="55.0" layoutY="610.0" mnemonicParsing="false" onAction="#cancelBtnClicked" text="Cancel" />
  </children>
</AnchorPane>
