<AnchorPane id="AnchorPane" prefHeight="432.0" prefWidth="499.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="icare.controllers.AddPatientViewController">
